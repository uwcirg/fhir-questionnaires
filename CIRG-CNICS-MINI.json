      "readOnly": true,
      "extension": [
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-calculatedExpression",
          "valueExpression": {
            "language": "text/fhirpath",
            "expression": "((%resource.item.where(linkId = 'MINI-0').answer.valueCoding.code.exists() and (%resource.item.where(linkId = 'MINI-0').answer.valueCoding.code = 'MINI-0-1')) or ((%resource.item.where(linkId = 'MINI-0').answer.valueCoding.code.exists() and (%resource.item.where(linkId = 'MINI-0').answer.valueCoding.code = 'MINI-0-0')) and (%resource.item.where(linkId = 'MINI-2').answer.valueCoding.code.exists() and %resource.item.where(linkId = 'MINI-3').answer.valueCoding.code.exists() and %resource.item.where(linkId = 'MINI-4').answer.valueCoding.code.exists() and %resource.item.where(linkId = 'MINI-5').answer.valueCoding.code.exists() and %resource.item.where(linkId = 'MINI-6').answer.valueCoding.code.exists() and %resource.item.where(linkId = 'MINI-7').answer.valueCoding.code.exists() and %resource.item.where(linkId = 'MINI-8').answer.valueCoding.code.exists())))"
